{
  "name": "react-portfolio",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "axios": "^0.16.2",
    "bulma": "^0.5.2",
    "gh-pages": "^1.0.0",
    "react-router": "^4.2.0",
    "react-scripts": "1.0.13"
  },
  "dependencies": {
    "history": "^4.7.2",
    "node-sass-chokidar": "0.0.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
